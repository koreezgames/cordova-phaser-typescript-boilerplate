{
  "name": "cordova-phaser-typescript-boilerplate",
  "author": "Koreez LLC <hello@koreez.com>",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=8.11.2",
    "npm": ">=6.2.0"
  },
  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "build": "npm run build-assets && npm run build-phaser && webpack --config config/webpack.config.js --env production --mode production --progress --colors",
    "build-phaser": "webpack --config config/webpack.phaser.config.js --display-modules --mode production --progress --colors",
    "build-ios": "npm run build && cordova prepare ios",
    "build-android": "npm run build && cordova prepare android",
    "stats": "webpack --config config/webpack.config.js --env production --mode production --profile --json > stats.json",
    "start": "npm run build-assets && npm run build-phaser && webpack-dev-server --config config/webpack.config.js --mode development --host 0.0.0.0",
    "start-device": "npm run build-assets && npm run build-phaser && webpack-dev-server --config config/webpack.config.js --env device --mode development --host 0.0.0.0",
    "build-assets": "node ./scripts/generateAssetsClass.js",
    "debug-assets": "node --nolazy --inspect-brk=9229 ./scripts/generateAssetsClass.js",
    "build-quick": "webpack --config config/webpack.config.js --env production --mode development --progress --colors",
    "build-quick-ios": "npm run build-quick && cordova prepare ios",
    "build-quick-android": "npm run build-quick && cordova prepare android",
    "serve": "npm run build && serve -p 3000 -s www",
    "build-firebase": "npm run build && firebase deploy --only hosting"
  },
  "devDependencies": {
    "@types/cordova": "0.0.34",
    "@types/node": "^10.12.18",
    "@types/webfontloader": "^1.6.29",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "directory-tree": "^2.2.1",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "exports-loader": "^0.7.0",
    "imports-loader": "^0.8.0",
    "phaser": "^3.16.0-rc2",
    "prettier": "^1.16.1",
    "serve": "^10.1.1",
    "shelljs": "^0.8.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-consistent-codestyle": "^1.15.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tsutils": "^3.7.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@koreez/phaser3-i18n": "^2.0.5",
    "@koreez/phaser3-ninepatch": "^1.2.3",
    "@robotlegsjs/core": "^1.0.0",
    "@robotlegsjs/macrobot": "^1.0.0",
    "@robotlegsjs/phaser": "^1.0.1",
    "@robotlegsjs/signalcommandmap": "^1.0.0",
    "@robotlegsjs/signals": "^1.0.0",
    "cordova-custom-config": "^5.0.3",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-cocoapod-support": "^1.6.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-fullscreen": "^1.2.0",
    "cordova-plugin-multidex": "^0.1.4",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "mobx": "^5.9.0",
    "reflect-metadata": "^0.1.13",
    "webfontloader": "^1.6.28"
  },
  "cordova": {
    "platforms": [
      "ios"
    ],
    "plugins": {
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-custom-config": {},
      "cordova-plugin-fullscreen": {},
      "cordova-plugin-device": {}
    }
  }
}
